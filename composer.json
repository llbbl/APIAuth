{
  "name": "eig/apiauth",
  "description": "An extended JWT authentication system for JSON API's in PHP",
  "license": "MIT",
  "keywords": ["api", "json, jwt, authentication"],
  "type": "library",
  "homepage": "https://github.com/excellentingenuity/APIAuth",
  "authors": [
    {
      "name": "James Johnson",
      "email": "james.johnson@excellentingenuity.com",
      "role": "Developer/Maintainer"
    }
  ],
  "support": {
    "docs": "https://github.com/excellentingenuity/APIAuth",
    "issues": "https://github.com/excellentingenuity/APIAuth/issues",
    "wiki": "https://github.com/excellentingenuity/APIAuth/wiki"
  },
  "require": {
    "php": ">=5.6",
    "eig/Configurator": "^1.1",
    "lcobucci/jwt": "^3.1"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.2",
    "mockery/mockery": "^0.9.4",
    "satooshi/php-coveralls": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "eig\\APIAuth\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "eig\\APIAuth\\Test\\": "tests"
    }
  }
}
